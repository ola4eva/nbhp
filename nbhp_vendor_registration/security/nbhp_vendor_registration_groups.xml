<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <record id="category_vendor_registration" model="ir.module.category">
            <field name="name">Vendor Registration</field>
        </record>

        <record id="group_vendor_reg_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="category_vendor_registration" />
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]" />
            <field name="users" eval="[(4, ref('base.user_admin'))]" />
        </record>

        <record id="group_vendor_reg_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="category_vendor_registration" />
            <field name="implied_ids"
                eval="[(4, ref('nbhp_vendor_registration.group_vendor_reg_user'))]" />
            <field name="users" eval="[(4, ref('base.user_admin'))]" />
        </record>

    </data>


</odoo>